#!groovy

pipeline {
  agent any
  options {
    skipDefaultCheckout()
    ansiColor('xterm')
    timestamps()
    //timeout(time: 60, unit: 'MINUTES')
    //disableConcurrentBuilds()
  }
  stages {
    stage('reload') {
      steps {
        build job: 'reload-dev'
      }
    }
    stage('deploy') {
      steps {
        build job: 'deploy-ENV', parameters: [booleanParam(name: 'RECONFIGURE_PIPELINE', value: false), string(name: 'environment', value: 'dev')]
      }
    }
  }     
}
